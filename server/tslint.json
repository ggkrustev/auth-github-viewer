{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": ["node_modules/**/*.ts"]
    },
    "jsRules": false,
    "rules": {
        "interface-name": ["never-prefix"],
        "object-literal-sort-keys": false,
        "ordered-imports": false
    },
    "rulesDirectory": []
}
